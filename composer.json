{
    "name": "marrios/mareaturborouter",
    "description": "Route manager",
    "keywords": ["Router", "MVC", "Manager"],
    "license": "GPL-3.0-only",
    "type": "library",
    "autoload": {
        "psr-4": {
            "MareaTurbo\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Marrios Dev",
            "email": "edmariooliver@outlook.com"
        }
    ],
    "require": {
        "php": ">=8.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "pestphp/pest": "^2.23",
        "mockery/mockery": "^1.6"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
